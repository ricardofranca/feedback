{
  "name": "feedback",
  "version": "0.0.1",
  "description": "Um produto para obter Feedbacks e atingir objectivos pessoais.",
  "private": true,
  "engines": {
    "node": "5.11.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cmilfont/feedback.git"
  },
  "author": "cmilfont",
  "license": "Copyright",
  "bugs": {
    "url": "https://github.com/cmilfont/feedback/issues"
  },
  "homepage": "http://prfeedback.herokuapp.com/",
  "scripts": {
    "postinstall": "parallelshell 'npm run deploy:js' 'sequelize db:migrate'",
    "db:create": "node_modules/.bin/create_database",
    "db:drop": "node_modules/.bin/drop_database",
    "verify:js": "./node_modules/eslint/bin/eslint.js ./app --ext .jsx --ext .js",
    "watch:css": "stylus -w -u ./node_modules/poststylus --with \"['postcss-opacity','postcss-cssnext']\" styles/main.styl -o public/assets/css/main.css",
    "watch:js": "watchify app/app.jsx -m -d -v -o public/assets/js/bundle.js",
    "deploy:js": "browserify app/app.jsx -o public/assets/js/bundle.js -t babelify -t [uglifyify --no-sourcemap --no-bundle-external]",
    "build:js": "browserify app/app.jsx -o public/assets/js/bundle.min.js -t babelify -t [uglifyify --no-sourcemap --no-bundle-external]",
    "watch:all": "parallelshell 'npm run watch:js' 'npm run watch:css' 'npm run offline' ",
    "dev": "parallelshell 'npm start' 'npm run watch:all' ",
    "start": "node -r babel-register index.js",
    "start:dev": "nodemon -V -i 'app' -i 'public' -i 'styles' -i 'views' -r babel-register index.js",
    "offline": "node -r babel-register update_version.js ",
    "test": "mocha test/.setup.js test/**/*Test.jsx"
  },
  "browserify": {
    "transform": [
      "babelify",
      "uglifyify"
    ]
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-resolver": "0.0.6",
    "babel-plugin-transform-decorators": "^6.6.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.0",
    "browserify": "^13.0.0",
    "connect-flash": "^0.1.1",
    "connect-multiparty": "^2.0.0",
    "cookie-parser": "^1.4.1",
    "csurf": "^1.9.0",
    "elasticsearch": "^11.0.1",
    "envify": "^3.4.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^1.3.1",
    "express": "^4.13.4",
    "express-react-views": "^0.10.0",
    "express-session": "^1.13.0",
    "faker": "^3.1.0",
    "imagemin": "^5.1.1",
    "imagemin-cli": "^2.1.0",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "material-ui": "^0.15.2",
    "moment": "^2.12.0",
    "morgan": "^1.7.0",
    "parallelshell": "^2.0.0",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "passport-local": "^1.0.0",
    "pg": "^4.5.1",
    "pg-hstore": "^2.3.2",
    "postcss-cssnext": "^2.7.0",
    "postcss-opacity": "^4.0.0",
    "poststylus": "^0.2.3",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-ga": "*",
    "react-hammerjs": "github:cmilfont/react-hammerjs",
    "react-markdown": "*",
    "react-redux": "^4.4.5",
    "react-redux-saga": "github:cmilfont/react-redux-saga",
    "react-router": "*",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-api": "^0.9.8",
    "redux-react-local": "^1.4.0",
    "redux-saga": "^0.11.0",
    "redux-thunk": "^2.1.0",
    "sendgrid": "^4.0.1",
    "sequelize": "^3.24.1",
    "sequelize-cli": "^2.4.0",
    "sequelize-migration-pg-extras": "github:cmilfont/sequelize-migration-pg-extras",
    "serve-favicon": "^2.3.0",
    "stylus": "^0.54.2",
    "uglifyify": "^3.0.1",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.3.1",
    "webpack": "^2.1.0-beta"
  },
  "devDependencies": {
    "babel-plugin-rewire": "^1.0.0-rc-7",
    "chai": "^3.5.0",
    "enzyme": "^2.4.1",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5"
  }
}
